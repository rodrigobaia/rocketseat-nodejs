// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db/dev.db"
}

model User{
  id          String @id @default(uuid())
  name        String
  github_id   Int
  avatar_url  String
  login       String
  company     String
  blog        String
  location    String
  email       String

  @@map("users")
}